FROM AsignacionFacturas             ASINGFAC 
	LEFT JOIN Ped_Facturas          PEDFACTURAS     WITH(NOLOCK) ON (ASINGFAC.idFactura            = PEDFACTURAS.idFactura)
	LEFT JOIN Ped_Guias             PEDGUIAS        WITH(NOLOCK) ON (PEDFACTURAS.idGuia            = PEDGUIAS.idGuia)
	LEFT JOIN Transportistas        PEDGUIASTRM     WITH(NOLOCK) ON (PEDGUIAS.idTransportMaster    = PEDGUIASTRM.idTransportista)				
	LEFT JOIN Transportistas        PEDGUIASTRH     WITH(NOLOCK) ON (PEDGUIAS.idTransportMaster    = PEDGUIASTRH.idTransportista)				
	LEFT JOIN Operaciones           PEDGUIASOP      WITH(NOLOCK) ON (PEDGUIAS.idOperacion          = PEDGUIASOP.idTipoOperacion)
	LEFT JOIN Agencias              PEDGUIASAG      WITH(NOLOCK) ON (PEDGUIAS.idAgencia            = PEDGUIASAG.IdAgencia)
	LEFT JOIN Aduanas               PEDGUIASAGAD    WITH(NOLOCK) ON (PEDGUIASAG.idAgencia          = PEDGUIASAGAD.idAduana)
	LEFT JOIN Paises                PEDGUIASPTM     WITH(NOLOCK) ON (PEDGUIAS.idPaisTransporMaster = PEDGUIASPTM.idPais)
	LEFT JOIN Paises                PEDGUIASPTH     WITH(NOLOCK) ON (PEDGUIAS.idPaisTransporMaster = PEDGUIASPTH.idPais)
	LEFT JOIN ObservacionesGuia     PEDGUIASOG      WITH(NOLOCK) ON (PEDGUIAS.idGuiaObservacion    = PEDGUIASOG.idGuiaObservacion)
	LEFT JOIN PrioridadesGuia       PEDGUIASPG      WITH(NOLOCK) ON (PEDGUIAS.idPrioridad          = PEDGUIASPG.idPrioridad)
	LEFT JOIN Clientes              PEDGUIASCLI     WITH(NOLOCK) ON (PEDGUIAS.idCliente            = PEDGUIASCLI.idCliente)
	LEFT JOIN EntidadesFederativas  PEDGUIASCLIEF   WITH(NOLOCK) ON (PEDGUIASCLI.idEntidad         = PEDGUIASCLIEF.idEntidad)
	LEFT JOIN Paises                PEDGUIASCLIPAIS WITH(NOLOCK) ON (PEDGUIASCLI.idPais            = PEDGUIASCLIPAIS.idPais)
	LEFT JOIN Bancos                PEDGUIASBA      WITH(NOLOCK) ON (PEDGUIAS.AnticipoidBanco      = PEDGUIASBA.idBanco)
	LEFT JOIN Puertos               PEDGUIASPU      WITH(NOLOCK) ON (PEDGUIAS.idPuerto             = PEDGUIASPU.idPuerto)
	LEFT JOIN EstatusGuias  PEDGUIAESTGUIASENVDOC   WITH(NOLOCK) ON (PEDGUIAS.idGuia               = PEDGUIAESTGUIASENVDOC.idGuia AND PEDGUIAESTGUIASENVDOC.idClavesEstatusGuias = 2)
	LEFT JOIN EstatusGuias  PEDGUIAESTGUIASCAPPED   WITH(NOLOCK) ON (PEDGUIAS.idGuia               = PEDGUIAESTGUIASCAPPED.idGuia AND PEDGUIAESTGUIASCAPPED.idClavesEstatusGuias = 3)
	LEFT JOIN EstatusGuias  PEDGUIAESTGUIASREVPED   WITH(NOLOCK) ON (PEDGUIAS.idGuia               = PEDGUIAESTGUIASREVPED.idGuia AND PEDGUIAESTGUIASREVPED.idClavesEstatusGuias = 4)
	LEFT JOIN EstatusGuias  PEDGUIAESTGUIASREVCEM   WITH(NOLOCK) ON (PEDGUIAS.idGuia               = PEDGUIAESTGUIASREVCEM.idGuia AND PEDGUIAESTGUIASREVCEM.idClavesEstatusGuias = 5)
	LEFT JOIN EstatusGuias  PEDGUIAESTGUIASPROPRE   WITH(NOLOCK) ON (PEDGUIAS.idGuia               = PEDGUIAESTGUIASPROPRE.idGuia AND PEDGUIAESTGUIASPROPRE.idClavesEstatusGuias = 6)
	LEFT JOIN EstatusGuias  PEDGUIAESTGUIASPRODES   WITH(NOLOCK) ON (PEDGUIAS.idGuia               = PEDGUIAESTGUIASPRODES.idGuia AND PEDGUIAESTGUIASPRODES.idClavesEstatusGuias = 7)
	LEFT JOIN EstatusGuias  PEDGUIAESTGUIASENTCVA   WITH(NOLOCK) ON (PEDGUIAS.idGuia               = PEDGUIAESTGUIASENTCVA.idGuia AND PEDGUIAESTGUIASENTCVA.idClavesEstatusGuias = 11)
	LEFT JOIN EstatusGuias  PEDGUIAESTGUIASPRECAP   WITH(NOLOCK) ON (PEDGUIAS.idGuia               = PEDGUIAESTGUIASPRECAP.idGuia AND PEDGUIAESTGUIASPRECAP.idClavesEstatusGuias = 27)
	LEFT JOIN EstatusGuias  PEDGUIAESTGUIASCIECON   WITH(NOLOCK) ON (PEDGUIAS.idGuia               = PEDGUIAESTGUIASCIECON.idGuia AND PEDGUIAESTGUIASCIECON.idClavesEstatusGuias = 29)
	LEFT JOIN EstatusGuias  PEDGUIAESTGUIASPROTRA   WITH(NOLOCK) ON (PEDGUIAS.idGuia               = PEDGUIAESTGUIASPROTRA.idGuia AND PEDGUIAESTGUIASPROTRA.idClavesEstatusGuias = 31)
	LEFT JOIN Incoterms             PEDFACINCO      WITH(NOLOCK) ON (PEDFACTURAS.idIncoterm        = PEDFACINCO.idIncoterm)
	LEFT JOIN Paises                PEDFACPAISM     WITH(NOLOCK) ON (PEDFACTURAS.idPaisMoneda      = PEDFACPAISM.idPais)
	LEFT JOIN Proveedores           PEDFACPR        WITH(NOLOCK) ON (PEDFACTURAS.idProveedor       = PEDFACPR.idProveedor)
	LEFT JOIN EntidadesFederativas  PEDFACPREF      WITH(NOLOCK) ON (PEDFACPR.idEntidad            = PEDFACPREF.idEntidad)
	LEFT JOIN Paises                PEDFACPRPAIS    WITH(NOLOCK) ON (PEDFACPR.idPais               = PEDFACPRPAIS.idPais)
	LEFT JOIN Proveedores           PEDFACDES       WITH(NOLOCK) ON (PEDFACTURAS.idDestinatario    = PEDFACDES.idProveedor)
	LEFT JOIN EntidadesFederativas  PEDFACDESEF     WITH(NOLOCK) ON (PEDFACDES.idEntidad           = PEDFACDESEF.idEntidad)
	LEFT JOIN Paises                PEDFACDESPAIS   WITH(NOLOCK) ON (PEDFACDES.idPais              = PEDFACDESPAIS.idPais)
	LEFT JOIN MetodosValoracion     PEDFACMV        WITH(NOLOCK) ON (PEDFACTURAS.idValoracion      = PEDFACMV.IdValoracion)
	LEFT JOIN Clientes              PEDFACCLI       WITH(NOLOCK) ON (PEDFACTURAS.idCliente         = PEDFACCLI.idCliente)
	LEFT JOIN EntidadesFederativas  PEDFACCLIEF     WITH(NOLOCK) ON (PEDFACCLI.idEntidad           = PEDFACCLIEF.idEntidad)
	LEFT JOIN Paises                PEDFACCLIPAIS   WITH(NOLOCK) ON (PEDFACCLI.idPais              = PEDFACCLIPAIS.idPais)
	LEFT JOIN Agencias              PEDFACAG        WITH(NOLOCK) ON (PEDFACTURAS.idAgencia         = PEDFACAG.IdAgencia)
	LEFT JOIN Aduanas               PEDFACAGAD      WITH(NOLOCK) ON (PEDFACAG.idAduana             = PEDFACAGAD.idAduana)
	LEFT JOIN Monedas_Equivalencias PEDFACMONEQ     WITH(NOLOCK) ON (PEDFACTURAS.idEquivalencia    = PEDFACMONEQ.idEquivalencia)
	LEFT JOIN EstatusFactura        PEDFACESTFAC    WITH(NOLOCK) ON (PEDFACTURAS.idEstatus         = PEDFACESTFAC.idEstatus)
	LEFT JOIN Paises                PEDFACPAISPROV  WITH(NOLOCK) ON (PEDFACTURAS.idPaisProveedor   = PEDFACPAISPROV.idPais)
